<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane maxHeight="200.0" maxWidth="479.0" prefHeight="200.0" prefWidth="479.0" styleClass="darkBlue" stylesheets="@SectionWindowCss.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="180.0" styleClass="darkBlue" stylesheets="@SectionWindowCss.css">
         <children>
            <ComboBox fx:id="dayComboBox" layoutX="17.0" layoutY="90.0" prefWidth="150.0" promptText="Days" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" />
            <Label layoutX="76.0" layoutY="39.0" text="Day" textFill="#f1f1f2">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="183.0" prefHeight="200.0" prefWidth="250.0" styleClass="darkBlue" stylesheets="@SectionWindowCss.css">
         <children>
            <ComboBox fx:id="startHourComboBox" layoutX="34.0" layoutY="91.0" prefHeight="25.0" prefWidth="25.0" promptText="2" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" />
            <Label layoutX="139.0" layoutY="36.0" text="Time" textFill="#f1f1f2">
               <font>
                  <Font name="System Bold Italic" size="20.0" />
               </font>
            </Label>
            <Label layoutX="203.0" layoutY="64.0" text="End" textFill="#f1f1f2">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="79.0" layoutY="64.0" text="Start" textFill="#f1f1f2">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <ComboBox fx:id="startMinComboBox" layoutX="90.0" layoutY="91.0" prefHeight="25.0" prefWidth="59.0" promptText="00" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" />
            <ComboBox fx:id="EndHourComboBox" layoutX="160.0" layoutY="91.0" prefHeight="25.0" prefWidth="42.0" promptText="2" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" />
            <ComboBox fx:id="endMinComboBox" layoutX="216.0" layoutY="91.0" prefHeight="25.0" prefWidth="59.0" promptText="00" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" />
            <Button fx:id="commitBtn" layoutX="224.0" layoutY="161.0" mnemonicParsing="false" onAction="#commitBtn" styleClass="lightGrey" stylesheets="@SectionWindowCss.css" text="Commit" />
         </children>
      </AnchorPane>
      <HBox layoutX="10.0" layoutY="10.0" prefHeight="20.0" prefWidth="37.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button contentDisplay="GRAPHIC_ONLY" maxHeight="40.0" maxWidth="40.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" styleClass="stageControl" stylesheets="@SectionWindowCss.css" text="Button">
               <graphic>
                  <FontIcon iconLiteral="fa-window-minimize" iconSize="10" styleClass="iconsControl" />
               </graphic>
            </Button>
            <Button contentDisplay="GRAPHIC_ONLY" maxHeight="40.0" maxWidth="40.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" onAction="#closeStage" styleClass="stageControl" stylesheets="@SectionWindowCss.css" text="Button">
               <graphic>
                  <FontIcon iconLiteral="fa-times" iconSize="15" styleClass="iconsControl" />
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
